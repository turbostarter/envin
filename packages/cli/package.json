{
  "name": "@envin/cli",
  "version": "1.1.12",
  "description": "Type-safe env validation with live previews",
  "keywords": [
    "turbostarter",
    "environment variables",
    "validation",
    "zod",
    "arktype",
    "valibot"
  ],
  "homepage": "https://envin.turbostarter.dev",
  "bugs": {
    "url": "https://github.com/turbostarter/envin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbostarter/envin.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": "Bartosz Zagrodzki",
  "bin": {
    "envin": "dist/cli/index.mjs"
  },
  "scripts": {
    "build": "tsup-node && bun run build:preview",
    "build:preview": "node ./scripts/build-preview-server.mjs",
    "clean": "rm -rf dist .turbo node_modules .next",
    "dev": "tsup-node --watch",
    "dev:preview": "cd ../../apps/example && tsx ../../packages/cli/src/cli/index.ts dev",
    "start": "node dist/cli/index.mjs",
    "typecheck": "tsc --noEmit",
    "prepack": "bun ../../scripts/replace-workspace-protocol.ts && bun ../../scripts/populate-readme.ts"
  },
  "dependencies": {
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-slot": "1.2.4",
    "radix-ui": "catalog:",
    "@svgr/webpack": "^8.1.0",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "consola": "^3.4.2",
    "debounce": "^2.2.0",
    "dotenv": "^16.5.0",
    "envin": "workspace:*",
    "esbuild": "^0.25.10",
    "log-symbols": "^7.0.0",
    "mime-types": "^3.0.1",
    "next": "catalog:",
    "ora": "^8.2.0",
    "react-hook-form": "^7.62.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tsconfig-paths": "^4.2.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@swc/core": "1.11.21",
    "@tailwindcss/postcss": "catalog:",
    "@types/babel__core": "7.20.5",
    "@types/babel__traverse": "7.20.7",
    "@types/mime-types": "2.1.4",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "class-variance-authority": "catalog:",
    "clsx": "catalog:",
    "lucide-react": "catalog:",
    "postcss": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "source-map-js": "1.2.1",
    "stacktrace-parser": "0.1.11",
    "tailwind-merge": "catalog:",
    "tailwindcss": "catalog:",
    "tsup": "catalog:",
    "tsx": "^4.19.4",
    "tw-animate-css": "catalog:",
    "typescript": "catalog:"
  }
}
