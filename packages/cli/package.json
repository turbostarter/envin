{
  "name": "@envin/cli",
  "version": "1.0.0",
  "description": "Type-safe env validation with live previews",
  "keywords": [
    "turbostarter",
    "environment variables",
    "validation",
    "zod",
    "arktype",
    "valibot"
  ],
  "homepage": "https://envin.turbostarter.dev",
  "bugs": {
    "url": "https://github.com/turbostarter/envin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbostarter/envin.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": "Bartosz Zagrodzki",
  "bin": {
    "cli": "dist/cli/index.mjs"
  },
  "scripts": {
    "build": "tsup-node && bun run build:preview",
    "build:preview": "node ./scripts/build-preview-server.mjs",
    "clean": "rm -rf dist .turbo node_modules .next",
    "dev": "tsup-node --watch",
    "dev:preview": "cd ../../apps/example && tsx ../../packages/cli/src/cli/index.ts dev",
    "start": "node dist/cli/index.mjs",
    "typecheck": "tsc --noEmit",
    "prepack": "bun ../../scripts/replace-workspace-protocol.ts && bun ../../scripts/populate-readme.ts"
  },
  "dependencies": {
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.6",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle": "^1.1.6",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@svgr/webpack": "^8.1.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dotenv": "^16.5.0",
    "envin": "workspace:*",
    "esbuild": "^0.25.0",
    "log-symbols": "^7.0.0",
    "mime-types": "^3.0.1",
    "next": "^15.3.3",
    "ora": "^8.2.0",
    "react-hook-form": "^7.56.4",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@rjsf/core": "^6.0.0-beta.1",
    "@rjsf/shadcn": "^6.0.0-beta.1",
    "@rjsf/utils": "^6.0.0-beta.1",
    "@rjsf/validator-ajv8": "^6.0.0-beta.1",
    "@swc/core": "1.11.21",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/babel__core": "7.20.5",
    "@types/babel__traverse": "7.20.7",
    "@types/mime-types": "2.1.4",
    "@types/node": "22.14.1",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.507.0",
    "postcss": "^8.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "source-map-js": "1.2.1",
    "stacktrace-parser": "0.1.11",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.0.12",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.2.9",
    "typescript": "5.8.3"
  }
}
